modules = ["nodejs-20", "web", "python-3.11", "postgresql-16"]
run = "npx tsx watch server/index.ts"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-24_05"
packages = ["ffmpeg", "libwebp"]

[deployment]
deploymentTarget = "cloudrun"
buildCommand = "npm run build"
run = ["sh", "-c", "node dist/server/index.js"]
build = ["sh", "-c", "npm run build"]

[agent]
integrations = ["javascript_stripe:1.0.0", "javascript_gemini_ai_integrations:1.0.0", "javascript_database:1.0.0"]

[[ports]]
localPort = 5000
externalPort = 80

